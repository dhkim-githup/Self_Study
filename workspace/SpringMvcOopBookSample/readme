CREATE  TABLE BOARD (
  seq 		number(5) not null,
  title 	VARCHAR2(255) NOT NULL ,
  content 	VARCHAR2(1000) NOT NULL ,
  writer 	VARCHAR2(10) NOT NULL ,
  password	number(5) NOT NULL ,
  regDate 	date NOT NULL ,
  cnt 		number(5)
);



